global:
  checkNewVersion: false
  sendAnonymousUsage: false

log:
  level: ERROR

api:
  dashboard: true

entryPoints:
  web:
    address: :80
  websecure:
    address: :443

providers:
  docker:
    network: web

certificatesResolvers:
  tlsResolver:
    acme:
      email: "{{ traefik_acme_email }}"
      storage: "acme.json"
      httpChallenge:
        entryPoint: "web"

